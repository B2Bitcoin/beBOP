{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"yaml.schemas": {
		"https://json.schemastore.org/github-workflow.json": ".github/workflows/deploy.yml"
	},
	"githubPullRequests.ignoredPullRequestBranches": ["main", "main"],
	"i18n-ally.keystyle": "nested",
	"i18n-ally.localesPaths": [
		"src/lib/translations",
		"src/routes/script/language",
		"src/routes/(app)/admin[[hash=admin_hash]]/language",
		"static/tinymce/plugins/help/js/i18n"
	],
	"i18n-ally.sourceLanguage": "en",
	"i18n-ally.tabStyle": "tab",
	"i18n-ally.sortKeys": true,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
