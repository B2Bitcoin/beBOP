{
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"yaml.schemas": {
		"https://json.schemastore.org/github-workflow.json": ".github/workflows/deploy.yml"
	},
	"githubPullRequests.ignoredPullRequestBranches": ["main", "main"],
	"i18n-ally.keystyle": "nested",
	"i18n-ally.localesPaths": ["src/lib/translations"],
	"i18n-ally.sourceLanguage": "en",
	"i18n-ally.tabStyle": "tab",
	"i18n-ally.sortKeys": true,
	"[typescript]": {
		"editor.defaultFormatter": "hb432.prettier-eslint-typescript"
	}
}
